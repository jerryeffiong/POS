  <DataSets>
    <DataSet Name="POSDS_ItemsTotal">
      <Query>
        <DataSourceName>dbConnectionString</DataSourceName>
        <CommandText>SELECT        Items.Barcode, Items.ItemName, SUM((ReciptDetails.ItemSellPrice - ReciptDetails.ItemBuyPrice) * ReciptDetails.ItemCount) AS Profit, 
                         SUM(ReciptDetails.ItemCount) AS TotalCount
FROM            (Items INNER JOIN
                         ReciptDetails ON Items.Barcode = ReciptDetails.Barcode)
GROUP BY Items.Barcode, Items.ItemName</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Barcode">
          <DataField>Barcode</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ItemName">
